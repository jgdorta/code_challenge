        <Request>
            <Step>
                <Name>AM-Path-Suffix-False</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response>
            <Step>
                <Name>EV-Request</Name>
            </Step>
            <Step>
                <Name>AM-Response</Name>
            </Step>
        </Response>
    </PostFlow>
    <Flows/>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables name="EV-Request" async="false" continueOnError="false" enabled="true">
    <DisplayName>EV-Request</DisplayName>
    <Properties/>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <JSONPayload>
        <Variable name="id">
            <JSONPath>$.id</JSONPath>
        </Variable>
        <Variable name="name">
            <JSONPath>$.name</JSONPath>
        </Variable>
        <Variable name="email">
            <JSONPath>$.email</JSONPath>
        </Variable>
        <Variable name="birthDate">
            <JSONPath>$.birthDate</JSONPath>
        </Variable>
        <Variable name="address">
            <JSONPath>$.address</JSONPath>
        </Variable>
    </JSONPayload>
    <Source clearPayload="false">request</Source>
    <VariablePrefix>user</VariablePrefix>
</ExtractVariables>